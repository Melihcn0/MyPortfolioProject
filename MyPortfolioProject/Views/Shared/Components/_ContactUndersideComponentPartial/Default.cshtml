@model List<Contact>

    <div class="col-four tab-full contact__infos">
        @foreach (var item in Model)
        {
            <h4 class="h06">Telefon</h4>
            <p>
                @item.Phone1<br>
                @item.Phone2
            </p>

            <h4 class="h06">Email</h4>
            <p>
                @item.Email1<br>
                @item.Email2
            </p>

            <h4 class="h06">Adres</h4>
            <p>
                @item.Address
            </p>
        }
        </div>